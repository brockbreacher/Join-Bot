{
    "name": "Join-Bot",
    "description": "Discord Join-Logs, Made Easy!",
    "repository": "https://github.com/brockbreacher/Join-Bot",
    "logo": "https://brbr.xyz/wp-content/uploads/2021/03/BrockDoorIcon-1.png",
   "image": "heroku/nodejs",
    "buildpacks": [
      {
        "url": "heroku/nodejs"
      }
    ],
   "env": {
      "TOKEN": {
        "description": "Discord Bot Token",
        "required": "true"
      },
      "ACTIVITY": {
        "description": "Bot Activity Message (EX, You Sleep)",
        "required": "true"
      },
      "TYPE": {
        "description": "The actvity type (LISTENING, WATCHING, PLAYING)",
        "required": "true"
      }
    }
 }
